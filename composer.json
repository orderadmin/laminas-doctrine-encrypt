{
    "name": "somecoding/laminas-doctrine-encrypt",
    "description": "Provides property encryption and hashing for Doctrine Entities when used within Laminas",
    "keywords": [
        "zf3",
        "doctrine2",
        "encrypt",
        "module",
        "zend framework 3",
        "laminas",
        "encryption"
    ],
    "homepage": "https://keet.me/",
    "license": "MIT",
    "authors": [
        {
            "name": "Dwarfex",
            "email": "dwarfex@somecoding.de",
            "homepage": "https://somecoding.de"
        }
    ],
    "require": {
        "php": " ~7.2 || 8.0.*",
        "ext-sodium": "*",
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6",
        "doctrine/doctrine-module": "^4.1.1",
        "paragonie/halite": "^4.4",
        "paragonie/hidden-string": "^2.0",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-servicemanager": "^2.7.8 || ^3.3",
        "laminas/laminas-stdlib": "^3.2",
        "laminas/laminas-dependency-plugin": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Keet\\Encrypt\\": "src/"
        },
        "classmap": [
            "src/Module.php"
        ]
    }
}
